<body>
	<section class="page-header">
		<h1 class="project-name">Session {{session}}: {{title}}</h1>
		<h2 class="project-tagline">{{tagline}}</h2>
	</section>
	<section class="main-content">
		{{{contents}}}
		<p>
			<a href="../index.html">&larr; Return to homepage</a>
		</p>
		<ol class="week-list">
