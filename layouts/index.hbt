<!doctype html>
<html lang="en">
<head>
	<meta charset="UTF-8" />
	<title>{{title}}</title>
	<link rel="stylesheet" href="styles/web.css"/>
	<script>
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
		ga('create', 'UA-69813045-1', 'auto');
		ga('send', 'pageview');
	</script>
</head>
<body>
	<section class="page-header">
		<h1 class="project-name">{{title}}</h1>
		<h2 class="project-tagline">{{tagline}}</h2>
	</section>
	<section class="main-content">
		{{{contents}}}
		{{#each collections.sessions}}
			<h1>
				Session {{this.session}}:
				<a href="{{this.folder}}/overview.html">{{this.title}}</a>
			</h1>
			<p>{{this.tagline}}</p>
			<ol class="week-list">
				{{#each this.weeks}}
		    	<li>
						Week {{this.week}}:
						<a href="{{this.folder}}/notes.html">{{this.title}}</a>
					</li>
				{{/each}}
			</ol>
		{{/each}}
		{{>footer}}
	</section>
</body>
</html>
