<!doctype html>
<html lang="en">
<head>
	<meta charset="UTF-8" />
	<title>Hymns of the Faith | {{title}}</title>
	<link rel="stylesheet" href="../styles/web.css"/>
	{{>analytics}}
</head>
<body>
	<section class="page-header">
		<h1 class="project-name">{{title}}</h1>
	</section>
	<section class="main-content">
		{{{contents}}}
		<p>
			<a href="../index.html">&larr; Return to homepage</a>
		</p>
		<ul class="index-list">
			{{#each entries}}
				<li>
					<a href="../{{this.file}}">{{this.value}}</a>
				</li>
			{{/each}}
		</ul>
		{{>footer}}
	</section>
</body>
</html>
